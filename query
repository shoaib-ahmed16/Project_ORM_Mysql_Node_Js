 CREATE TABLE users ( id INT NOT NULL AUTO_INCREMENT, First_Name VARCHAR(45) NOT NULL, Last_Name VARCHAR(45) NOT NULL,email  VARCHAR(45) NOT NULL, phone  VARCHAR(45) NOT NULL, comments TEXT NOT NULL, status  VARCHAR(10) NOT NULL
  DEFAULT 'active', PRIMARY KEY (id));

